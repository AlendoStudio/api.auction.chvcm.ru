{
  "name": "@alendo/api.auction.chvcm.ru",
  "version": "1.0.0",
  "description": "https://auction.chvcm.ru backend",
  "main": "dist/index.js",
  "bin": {
    "api.auction.chvcm.ru-mkadmin": "dist/bin/mkadmin.js",
    "api.auction.chvcm.ru-mkbucket": "dist/bin/mkbucket.js",
    "api.auction.chvcm.ru-schemaup": "dist/bin/schemaup.js",
    "api.auction.chvcm.ru-test-email": "dist/bin/test-email.js",
    "api.auction.chvcm.ru-web": "dist/bin/web.js"
  },
  "scripts": {
    "build": "yarn build:apidoc && yarn build:email && yarn build:ts",
    "build:apidoc": "yarn clean:apidoc && apidoc -i routes -i src -o apidoc",
    "build:email": "gulp",
    "build:ts": "yarn clean:ts && tsc -p tsconfig.production.json",
    "clean": "yarn clean:apidoc && yarn clean:email && yarn clean:ts",
    "clean:apidoc": "rimraf apidoc",
    "clean:email": "gulp clean",
    "clean:ts": "rimraf dist",
    "lint": "eslint --ext .ts,.js --ignore-path ../../.gitignore --max-warnings 0 .",
    "loc": "sloc index.ts global.ts bin routes src types",
    "mkadmin": "node dist/bin/mkadmin.js",
    "mkadmin:dev": "node $NODE_DEBUG_OPTION -r ts-node/register bin/mkadmin.ts",
    "mkbucket": "node dist/bin/mkbucket.js",
    "mkbucket:dev": "node $NODE_DEBUG_OPTION -r ts-node/register bin/mkbucket.ts",
    "schemaup": "node dist/bin/schemaup.js",
    "schemaup:dev": "node $NODE_DEBUG_OPTION -r ts-node/register bin/schemaup.ts",
    "test": "yarn clean:ts && mocha",
    "test-email": "node dist/bin/test-email.js",
    "test-email:dev": "node $NODE_DEBUG_OPTION -r ts-node/register bin/test-email.ts",
    "test:cov": "yarn clean:ts && nyc mocha",
    "web": "node dist/bin/web.js",
    "web:dev": "node $NODE_DEBUG_OPTION -r ts-node/register bin/web.ts"
  },
  "apidoc": {
    "name": "Интернет-аукцион для юридических лиц",
    "version": "1.0.0",
    "description": "Документация RESTful и WebSocket API",
    "url": "/api/v1",
    "template": {
      "forceLanguage": "ru"
    }
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "apidoc",
      "bin",
      "coverage",
      "dist",
      "test",
      "**/*.d.ts",
      "gulpfile.js"
    ],
    "reporter": [
      "lcov"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlendoStudio/api.auction.chvcm.ru.git"
  },
  "author": "Alendo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlendoStudio/api.auction.chvcm.ru/issues"
  },
  "homepage": "https://github.com/AlendoStudio/api.auction.chvcm.ru#readme",
  "dependencies": {
    "@alendo/public.api.auction.chvcm.ru": "1.0.0",
    "app-module-path": "^2.2.0",
    "aws-sdk": "^2.471.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "bytes": "^3.1.0",
    "celebrate": "^9.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cuid": "^2.1.6",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "email-templates": "^5.1.0",
    "express": "^4.17.1",
    "express-bearer-token": "^2.4.0",
    "express-promise-router": "^3.0.3",
    "express-recaptcha": "^5.0.0",
    "http-assert": "^1.4.1",
    "http-errors": "^1.7.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "marked": "^0.6.2",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "nodemailer": "^6.2.1",
    "otplib": "^11.0.1",
    "pg": "^7.11.0",
    "pkg-dir": "^4.2.0",
    "postgres-interval": "^1.2.0",
    "pug": "^2.0.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.8.8",
    "sequelize-typescript": "^1.0.0-beta.3",
    "socket.io": "^2.2.0",
    "socket.io-redis": "^5.2.0",
    "validator": "^11.0.0",
    "yargs": "^13.2.4",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@types/app-module-path": "^2.2.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.17.0",
    "@types/bytes": "^3.0.0",
    "@types/chai": "^4.1.7",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.5",
    "@types/debug": "^4.1.4",
    "@types/dotenv": "^6.1.1",
    "@types/email-templates": "^3.5.1",
    "@types/express": "^4.17.0",
